<%- include("../partials/header") %>

	<div class="container">
		<h1>Create a New Campground</h1>
		<div class="newdataform">
			
			<form action="/campgrounds" method="POST">
				<div class="form-group">
					<label for="exampleInputEmail1">Campground Name</label>
					<input type="text" name="campgroundName" class="form-control" id="exampleInputEmail1">
					<small id="emailHelp" class="form-text text-muted">We'll never share your information with anyone else.</small>
				</div>
				<div class="form-group">
					<label for="exampleInputPassword1">Image Url</label>
					<input type="text" name="imageUrl" class="form-control" id="exampleInputPassword1">
				</div>
				<div class="form-group">
					<label for="description">Description</label>
					<input type="text" name="description"  class="form-control" id="description">
				</div>
				<button type="submit" class="btn btn-primary btn-block mb-2">Submit</button>
				
			</form>
			<a href="/campgrounds">Go Back</a>
		</div>

	</div>

	<%- include("../partials/footer") %>